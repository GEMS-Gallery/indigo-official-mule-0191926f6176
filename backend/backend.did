type Result = 
 variant {
   err: text;
   ok;
 };
type Gem = 
 record {
   description: text;
   githubUrl: text;
   id: text;
   thumbnail: text;
   title: text;
 };
service : {
  addGem: (Gem) -> (Result);
  getGems: () -> (vec Gem) query;
  removeGem: (text) -> (Result);
  updateGem: (Gem) -> (Result);
}
