type Result = 
 variant {
   err: text;
   ok;
 };
type Gem = 
 record {
   author: record {
             avatar: text;
             name: text;
           };
   description: text;
   githubUrl: text;
   id: text;
   thumbnail: text;
   title: text;
 };
service : {
  addGem: (Gem) -> (Result);
  getGems: () -> (vec Gem) query;
  updateGem: (Gem) -> (Result);
}
